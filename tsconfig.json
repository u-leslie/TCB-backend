{
  "compilerOptions": {
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,

  },
  "include": [
    "./*.js",
    "./**/*.ts",
    "test/**/*.ts",
  ],
  "exclude": [
    "./swagger",
  ],
  
}